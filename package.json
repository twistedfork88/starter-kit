{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "sample isomorphic react boilerplate",
  "scripts": {
    "client": "webpack --config ./webpack/client.config.js",
    "server": "webpack -w --config ./webpack/server.config.js",
    "start": "nodemon ./webpack/server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/twistedfork88/boilerplate.git"
  },
  "author": "Abinash",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/twistedfork88/boilerplate/issues"
  },
  "homepage": "https://github.com/twistedfork88/boilerplate#readme",
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "@babel/preset-stage-0": "^7.0.0",
    "@loadable/babel-plugin": "^5.7.2",
    "@loadable/webpack-plugin": "^5.7.1",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^2.1.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "nodemon": "^1.12.5",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^3.10.0",
    "webpack-node-externals": "^1.6.0",
    "webpack-shell-plugin": "^0.5.0"
  },
  "dependencies": {
    "@loadable/component": "^5.7.0",
    "@loadable/server": "^5.7.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.4",
    "express": "^4.16.2",
    "express-handlebars": "^3.0.2",
    "helmet-csp": "^2.7.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "serialize-javascript": "^1.4.0",
    "styled-components": "^4.1.3",
    "xxhashjs": "^0.2.2"
  }
}
